{
  "expo": {
    "name": "playground",
    "slug": "playground",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/config/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/config/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FCCB6F"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/config/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.julio_lpz.playground"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/config/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "96ef1e7f-65c4-4572-ac5c-2b1c7b08a7ad"
      }
    }
  }
}
